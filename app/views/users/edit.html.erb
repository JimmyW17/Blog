<%= form_for @user do |f|%>
<%= f.label :email %>
<%= f.text_field :email %>
<%= f.label :password %>
<%= f.text_field :password %>
<%= f.label :fname %>
<%= f.text_field :fname %>
<%= f.label :lname %>
<%= f.text_field :lname %>
<%= f.label :age %>
<%= f.number_field :age %>
<%= f.submit %>
<% end %>
